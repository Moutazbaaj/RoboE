void setup() {
  Serial.begin(115200);
  Serial.println("Emotion Bot with INMP441 Starting...");

	FastLED.addLeds<NEOPIXEL, LED_PIN>(leds, LED_COUNT);
	FastLED.clear(); FastLED.show();

  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println("OLED not found!");
    while (1);
  }
	display.invertDisplay(true);
  display.setRotation(0);
  display.clearDisplay();

	setMoodLEDs(currentMood, dBControll); 
  showMood(currentMood, dBControll);
	



  setupI2SMic();
}

void loop() {
  int32_t samples[SAMPLE_BUFFER_SIZE];
  size_t bytesRead;

  i2s_read(I2S_NUM_0, (void*)samples, sizeof(samples), &bytesRead, portMAX_DELAY);
  int sampleCount = bytesRead / sizeof(int32_t);

  // Normalize and compute average absolute amplitude
  uint64_t sum = 0;
  for (int i = 0; i < sampleCount; i++) {
    int32_t sample = samples[i] >> 14;  // downscale to 18-bit typical range
    sum += abs(sample);
  }
  uint32_t avg = sum / sampleCount;

  // Convert to dB for more intuitive thresholding (optional)
  float dB = 20.0 * log10((float)avg + 1); // avoid log(0)
  Serial.printf("Avg Amplitude: %lu  %.2f dB\n", avg, dB);

	// Assign float dB to double dBControll
	dBControll = (double)dB;

  Mood newMood = currentMood;
	

  // Thresholds based on dB 
// Mood thresholds mapped to dB level
if (dB < 20.0) {
  newMood = SLEEPY;           // Too quiet  maybe soundcheck or pre-party calm
} else if (dB < 30.0) {
  newMood = DISAPPOINTED;     // Room is dead  no vibes
} else if (dB < 40.0) {
  newMood = CONFOUNDED;       // Still confusingly quiet
} else if (dB < 50.0) {
  newMood = CONFUSED;         // Mild chatter or background noise
} else if (dB < 60.0) {
  newMood = GRINNING;         // Friendly environment, people starting to vibe
} else if (dB < 65.0) {
  newMood = HUGGING;          // Cozy music, warming up
} else if (dB < 70.0) {
  newMood = KISSING;          // Romantic or mellow dance zone
} else if (dB < 75.0) {
  newMood = HAPPY;            // Peak enjoyment, people dancing, smiling
} else if (dB < 80.0) {
  newMood = COLD;             // Loud but maybe too impersonal
} else if (dB < 85.0) {
  newMood = ANGRY;            // Getting too loud  club intensity
} else if (dB < 90.0) {
  newMood = SHOUTING;         // People need to yell to hear  classic party chaos
} else if (dB < 95.0) {
  newMood = TEARY;            // Emotionally overloaded or ultra loud
} else if (dB < 100.0){
  newMood = VOMITING;         // Noise-induced meltdown 
}


	showDB(dBControll);
	detectBeat(dBControll);
	setMoodLEDs(currentMood, dBControll); 


  // Change mood only if different and 2s has passed
if (newMood != currentMood && millis() - lastChange > 2000) {
  lastChange = millis();
  currentMood = newMood;

  Serial.print("Mood changed to: ");
  Serial.println(getMoodName(currentMood));
	
	showMood(currentMood, dBControll);




  //smoothMoodTransition(currentMood, dBControll);
}

  delay(150);
}

// I2S Setup for INMP441
void setupI2SMic() {
  const i2s_config_t i2s_config = {
    .mode = (i2s_mode_t)(I2S_MODE_MASTER | I2S_MODE_RX),
    .sample_rate = 16000,
    .bits_per_sample = I2S_BITS_PER_SAMPLE_32BIT,
    .channel_format = I2S_CHANNEL_FMT_ONLY_LEFT,
    .communication_format = I2S_COMM_FORMAT_I2S,
    .intr_alloc_flags = ESP_INTR_FLAG_LEVEL1,
    .dma_buf_count = 4,
    .dma_buf_len = 256,
    .use_apll = false,
    .tx_desc_auto_clear = false,
    .fixed_mclk = 0
  };

  const i2s_pin_config_t pin_config = {
    .bck_io_num = I2S_SCK,
    .ws_io_num = I2S_WS,
    .data_out_num = I2S_PIN_NO_CHANGE,
    .data_in_num = I2S_SD
  };

  i2s_driver_install(I2S_NUM_0, &i2s_config, 0, NULL);
  i2s_set_pin(I2S_NUM_0, &pin_config);
  i2s_zero_dma_buffer(I2S_NUM_0);
}
